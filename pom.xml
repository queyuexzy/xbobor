		<dependency>
			<groupId>jstl</groupId>
			<artifactId>jstl</artifactId>
			<version>1.2</version>
		</dependency>
	</dependencies>

	<profiles>
		<profile>
			<id>online</id>
			<properties>
				<package>war</package>
			</properties>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>

			<build>
				<finalName>xbobor</finalName>
				<resources>
					<resource>
						<directory>src/main/resources</directory>
						<!--<excludes>-->
							<!--<exclude>config/spring-dev/*.xml</exclude>-->
						<!--</excludes>-->
					</resource>
				</resources>

				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-compiler-plugin</artifactId>
						<version>3.1</version>
						<configuration>
							<source>${jdk.version}</source>
							<target>${jdk.version}</target>
						</configuration>
					</plugin>

					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-war-plugin</artifactId>
						<version>2.4</version>
						<configuration>
							<archive>
								<addMavenDescriptor>false</addMavenDescriptor>
							</archive>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
</project>
